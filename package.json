{
  "name": "serverless-pdf-filler",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "mkdir -p tmp && AWS_ACCESS_KEY_ID=S3RVER AWS_SECRET_ACCESS_KEY=S3RVER NODE_OPTIONS=--max_old_space_size=4096 ./node_modules/.bin/serverless offline start --host 0.0.0.0 --lambdaPort 3009 --location .webpack/service --stage local",
    "start:sut": "npm run start",
    "start:test": "wait-for-it/wait-for-it.sh localhost:3009 --timeout=120 && npm run test:it",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:all": "npm run test:unit && run-p -r start:*",
    "test:unit": "jest",
    "test:it": "jest --config jest.config.it.js"
  },
  "dependencies": {
    "@pdf-lib/fontkit": "^1.0.0",
