    "aws-sdk": "^2.678.0",
    "copy-webpack-plugin": "^8.0.0",
    "node-fetch": "^2.6.0",
    "pdf-lib": "^1.7.0",
    "pino": "^6.2.1",
    "serverless": "1.83.3",
